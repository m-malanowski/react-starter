{
  "name": "React starter",
  "version": "0.1.0",
  "description": "React starter",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Mateusz Malanowski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-malanowski/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/m-malanowski/react-starter/issues"
  },
  "homepage": "https://github.com/m-malanowski/react-starter",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod",
    "clean-dist": "rimraf dist/*",
    "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss}\"",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watchAll --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "express": "^4.17.3"
  }
}
